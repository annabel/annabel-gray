- provide(:title, "Work")
.work-block
  .container
    .work-row
      .project-column
        .light-blue.hover-tile-outer
          .hover-tile-container
            .hover-tile.hover-tile-visible
              = image_tag "gitlab-com.png"
              %button.btn.work-info-button
                .glyphicon.glyphicon-plus.light-blue
            .hover-tile.hover-tile-hidden.light-blue
              .tile-content
                .work-name GitLab.com
                = link_to "View Site", "https://about.gitlab.com/", class: "btn light-blue-btn", target: "_blank"
                .header Skills strengthened:
                .skills Middleman, Ruby, JavaScript
